{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "max-len": 0,
    "no-continue": 0,
    "no-param-reassign": ["error", { "props": false }],
    "no-restricted-syntax": 0
  }
}
